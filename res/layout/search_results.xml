<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DEF8F8"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="180dp"
        android:layout_height="165dp"
        android:src="@drawable/top_corner"></ImageView>

    <ImageButton
        android:id="@+id/back_btn"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:src="@drawable/back_dark_blue"
        android:scaleType="fitCenter"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="37dp"
        android:background="@android:color/transparent">
    </ImageButton>
    <TextView
        android:id="@+id/results_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/back_btn"
        android:text="Search Results"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="38dp"
        android:textSize="29.5sp"
        android:textColor="#157177"
        android:textStyle="bold">
    </TextView>

    <RelativeLayout
        android:id="@+id/filter_dropdown"
        android:layout_width="88dp"
        android:layout_height="38dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="25dp"
        android:layout_marginTop="6dp"
        android:layout_below="@+id/results_txt"
        android:background="@drawable/filter_box_rounded"
        android:clickable="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Filter"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="15dp"
            android:layout_centerVertical="true"
            android:textSize="16sp"
            android:textColor="#157177"
            android:textStyle="bold">
        </TextView>
        <ImageButton
            android:layout_width="12dp"
            android:layout_height="8.5dp"
            android:background="@drawable/dropdown_blue"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp">
        </ImageButton>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/box_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/filter_dropdown"
        android:layout_marginLeft="20dp">
        <RelativeLayout
            android:id="@+id/box_1_pic"
            android:layout_width="110dp"
            android:layout_height="105dp"
            android:background="@drawable/search_result_boxes">
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_toRightOf="@+id/box_1_pic"
            android:background="@drawable/rounded_corners_txt"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="40dp"
            android:paddingRight="12dp"
            android:paddingLeft="12dp"
            android:paddingTop="8dp">
            <TextView
                android:id="@+id/sample_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sample 1"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="14sp">
            </TextView>
            <TextView
                android:id="@+id/pos_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lead - Technology Officer"
                android:layout_below="@+id/sample_1"
                android:textSize="9sp"
                android:textColor="@color/black">
            </TextView>
            <RelativeLayout
                android:id="@+id/available_1"
                android:layout_width="8.5dp"
                android:layout_height="8.5dp"
                android:background="@drawable/available_circle"
                android:layout_below="@id/pos_1"
                android:layout_marginTop="4dp">
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Available"
                android:textColor="#A7DA8C"
                android:layout_toRightOf="@id/available_1"
                android:layout_below="@id/pos_1"
                android:textSize="8sp"
                android:textStyle="bold"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="5dp">
            </TextView>
            <TextView
                android:id="@+id/price_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="$500/Session"
                android:layout_alignParentRight="true"
                android:textColor="#F7E194"
                android:textStyle="bold"
                android:textSize="7sp"
                android:layout_marginTop="5dp">
            </TextView>
            <ImageButton
                android:id="@+id/fav_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/filled_fav_heart"
                android:layout_alignParentRight="true"
                android:layout_below="@id/price_1"
                android:layout_marginTop="7dp"
                android:layout_marginRight="11dp">
            </ImageButton>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/box_2"
        android:layout_width="match_parent"
